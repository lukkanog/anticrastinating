{
  "manifest_version": 3,
  "name": "Anticrastinating",
  "version": "1.0",
  "description": "A Chrome extension to limit time spent on specific websites.",
  "permissions": ["tabs", "history", "storage"],
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "128": "assets/logo-128.png"
  }
}
